{
  "name": "honestcash-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run transfer-libs && ng serve",
    "start:prod": "npm run transfer-libs && ng serve -c production",
    "start:dev": "npm run transfer-libs && ng serve -c development",
    "build": "ng build",
    "test": "ng test",
    "test:dev": "ng test --code-coverage --source-map",
    "test:dev:single-file": "ng test --main ./src/test-single.ts",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "transfer-libs": "bash scripts/transfer-libs.sh",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr:prod": "npm run transfer-libs && npm run build:client-and-server-bundles:prod && npm run compile:server",
    "build:ssr:dev": "npm run transfer-libs && npm run build:client-and-server-bundles:dev && npm run compile:server",
    "build:ssr:local": "npm run transfer-libs && npm run build:client-and-server-bundles:local && npm run compile:server",
    "build:client-and-server-bundles:prod": "ng build --prod --base-href /v2/ --deploy-url /v2/ && ng run honestcash-v2:server:production",
    "build:client-and-server-bundles:dev": "ng build --configuration development --base-href /v2/ --deploy-url /v2/ && ng run honestcash-v2:server:development",
    "build:client-and-server-bundles:local": "ng build --configuration development && ng run honestcash-v2:server:development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/platform-server": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@angular/service-worker": "^7.2.13",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-regular-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.2",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/express-engine": "0.0.0",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "angularx-qrcode": "^1.6.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "domino": "^2.1.3",
    "express": "^4.15.2",
    "js-sha3": "^0.8.0",
    "ngx-chips": "^2.0.0-beta.0",
    "ngx-script-loader": "^1.1.0",
    "ngx-toastr": "^10.0.4",
    "rxjs": "~6.3.3",
    "simple-bitcoin-wallet": "0.0.7",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.2",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "^7.1.4",
    "@editorjs/editorjs": "^2.14.0",
    "@editorjs/embed": "^2.2.1",
    "@editorjs/header": "^2.2.4",
    "@editorjs/image": "^2.3.1",
    "@editorjs/paragraph": "^2.5.1",
    "@types/grecaptcha": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^11.13.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.0"
  }
}
